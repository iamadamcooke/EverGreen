<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.cs2340.evergreen"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.gatech.cs2340.evergreen.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.gatech.cs2340.evergreen.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="edu.gatech.cs2340.evergreen.Success"
            android:label="@string/title_activity_success" >
        </activity>
        <activity
            android:name="edu.gatech.cs2340.evergreen.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="edu.gatech.cs2340.evergreen.AccountsActivity"
            android:label="@string/title_activity_accounts" >
        </activity>
    </application>

</manifest>
